{
  "name": "mind-image-compiler",
  "version": "1.0.0",
  "description": "Node Application of MindAR.js Image Compiler.",
  "author": "Brent Williams <functnal@gmail.com> (https://www.github.com/JustBarnt)",
  "contributors": [{"name": "Brent Williams <brent.williams@ddincmail.org> (https://www.github.com/DDincBrent)"}],
  "main": "image-compile.js",
  "scripts": {
    "test": "node image-compiler.js"
  },
  "engines": {"node":">=17.0.0"},
  "type": "module",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "rules": {
      "arrow-spacing": [
        "warn",
        {
          "before": true,
          "after": true
        }
      ],
      "brace-style": [
        "error",
        "allman",
        {
          "allowSingleLine": true
        }
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "comma-spacing": [
        "error",
        {
          "after": true
        }
      ],
      "curly": [
        "error",
        "multi",
        "consistent"
      ],
      "dot-location": [
        "error",
        "property"
      ],
      "handle-callback-err": "off",
      "indent": [
        "warn",
        "tab"
      ],
      "max-nested-callbacks": [
        "error",
        {
          "max": 4
        }
      ],
      "max-statements-per-line": [
        "error",
        {
          "max": 2
        }
      ],
      "no-console": "off",
      "no-empty-function": "error",
      "no-floating-decimal": "error",
      "no-inline-comments": "error",
      "no-shadow": [
        "error",
        {
          "allow": [
            "err",
            "resolve",
            "reject"
          ]
        }
      ],
      "no-unused-vars": "warn",
      "no-var": "warn",
      "no-lonely-if": "off",
      "no-undef": "off",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "prefer-const": "warn",
      "quotes": [
        "error",
        "single"
      ],
      "semi": "error",
      "no-trailing-spaces": "off",
      "space-before-blocks": [
        "error",
        "never"
      ],
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "never",
          "named": "never",
          "asyncArrow": "always"
        }
      ],
      "space-in-parens": [
        "error",
        "never"
      ],
      "yoda": "warn"
    }
  },
  "keywords": ["webar","node","nodejs","node application","js"],
  "license": "MIT",
  "dependencies": {
    "@msgpack/msgpack": "^1.12.2",
    "@tensorflow/tfjs": "^3.13.0",
    "fs-extra": "^10.1.0"
  },
  "homepage": "https://github.com/DDincBrent/mind-image-compiler-node#README.md",
  "bugs": {
	  "url": "https://github.com/DDincBrent/mind-image-compiler-node/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/JustBarnt/mind-image-compiler-node/"
	}
}
